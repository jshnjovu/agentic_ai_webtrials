# LeadGen Makeover Agent API Configuration
# Copy this file to .env and fill in your actual API keys

# Google Places API Configuration
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Yelp Fusion API Configuration
YELP_FUSION_API_KEY=your_yelp_fusion_api_key_here

# Rate Limiting Configuration (optional - defaults will be used if not set)
GOOGLE_PLACES_RATE_LIMIT_PER_MINUTE=100
YELP_FUSION_RATE_LIMIT_PER_DAY=5000

# API Timeout Settings (optional - defaults will be used if not set)
API_TIMEOUT_SECONDS=30

# Circuit Breaker Configuration (optional - defaults will be used if not set)
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_RECOVERY_TIMEOUT=60

# Application Configuration
DEBUG=False
